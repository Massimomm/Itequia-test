@model IEnumerable<Model.BookAuthor>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstPublished)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AuthorsSummary)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.authorName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstPublished)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AuthorsSummary)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.authorName)
        </td>
    </tr>
}

</table>
